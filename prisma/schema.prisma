// learn more about it in the docs: https://pris.ly/d/prisma-schema

// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id                         String                 @id @default(uuid())
  name                       String
  email                      String                 @unique
  password                   String
  createdHeaderInfo          Header_info[]
  createdNavbar              Navbar[]
  createdSlider              Slider[]
  createdServiceSection      ServiceSection[]
  createdCounter             Counter[]
  createdItsolution          Itsolution[]
  createdAboutPage           About_page[]
  createdServicePage         Service_page[]
  createdContactPage         Contact_page[]
  createdConsultentcyService Consultentcy_service[]
  createdFooter              Footer[]
}

model Header_info {
  id          String  @id @default(uuid())
  lang        String
  welcomeText String
  address     String
  user        User?   @relation(fields: [authorId], references: [id])
  authorId    String?
}

model Navbar {
  id               String         @id @default(uuid())
  lang             String
  logo             String
  menu             Navbar_menu[]
  contact          Navbar_contact @relation(fields: [navbar_contactId], references: [id])
  navbar_contactId String
  user             User?          @relation(fields: [authorId], references: [id])
  authorId         String?
}

model Navbar_menu {
  id       String  @id @default(uuid())
  label    String
  href     String
  navbar   Navbar? @relation(fields: [navbarId], references: [id])
  navbarId String?
}

model Navbar_contact {
  id      String   @id @default(uuid())
  title   String
  phone   String
  icon    String
  navbars Navbar[]
}

model Slider {
  id       String  @id @default(uuid())
  lang     String
  slides   Slide[]
  user     User?   @relation(fields: [authorId], references: [id])
  authorId String?
}

model Slide {
  id       String  @id @default(uuid())
  title    String
  text     String
  image    String
  slider   Slider? @relation(fields: [sliderId], references: [id])
  sliderId String?
}

model ServiceSection {
  id          String    @id @default(uuid())
  lang        String
  heading     String
  description String
  services    Service[]
  user        User?     @relation(fields: [authorId], references: [id])
  authorId    String?
}

model Service {
  id        String         @id @default(uuid())
  title     String
  image     String
  section   ServiceSection @relation(fields: [sectionId], references: [id], onDelete: Cascade)
  sectionId String
  types     ServiceType[]
}

model ServiceType {
  id        String  @id @default(uuid())
  value     String
  service   Service @relation(fields: [serviceId], references: [id], onDelete: Cascade)
  serviceId String
}

model Counter {
  id       String         @id @default(uuid())
  lang     String
  counters Counter_list[]
  user     User?          @relation(fields: [authorId], references: [id])
  authorId String?
}

model Counter_list {
  id        String   @id @default(uuid())
  image     String
  value     String
  label     String
  counter   Counter? @relation(fields: [counterId], references: [id])
  counterId String?
}

model Itsolution {
  id          String             @id @default(uuid())
  lang        String
  titleSmall  String
  titleBig    String
  description String
  worksTypes  WorksType[]
  images      Itsolution_image[]
  user        User?              @relation(fields: [authorId], references: [id])
  authorId    String?
}

model WorksType {
  id           String      @id @default(uuid())
  title        String
  description  String
  itsolution   Itsolution? @relation(fields: [itsolutionId], references: [id])
  itsolutionId String?
}

model Itsolution_image {
  id           String      @id @default(uuid())
  leftColumn   Json
  rightColumn  Json
  itsolution   Itsolution? @relation(fields: [itsolutionId], references: [id])
  itsolutionId String?
}

model Service_page {
  id           String         @id @default(uuid())
  lang         String
  title        String
  social       String
  list         String
  servicesData Servive_list[]
  user         User?          @relation(fields: [authorId], references: [id])
  authorId     String?
}

model Servive_list {
  id             String        @id @default(uuid())
  title          String
  subtitle       String
  image          String
  description    String
  servicePage    Service_page? @relation(fields: [service_pageId], references: [id])
  service_pageId String?
}

model About_page {
  id           String               @id @default(uuid())
  lang         String
  btn_text     String
  hero_text    String
  blocks       About_block[]
  device_block About_device_block[]
  user         User?                @relation(fields: [authorId], references: [id])
  authorId     String?
}

model About_block {
  id           String      @id @default(uuid())
  title        String
  description  String
  image        String
  aboutPage    About_page? @relation(fields: [about_pageId], references: [id])
  about_pageId String?
}

model About_device_block {
  id           String      @id @default(uuid())
  title        String
  description  String
  aboutPage    About_page? @relation(fields: [about_pageId], references: [id])
  about_pageId String?
}

model About_logo {
  id    String @id @default(uuid())
  name  String
  image String
}

model Contact_page {
  id                  String  @id @default(uuid())
  lang                String
  title               String
  phone_number_one    String
  phone_number_two    String
  phone_decr          String
  email               String
  email_decr          String
  location            String
  work_location       String
  subtitle            String
  name_label          String
  name_placeholder    String
  phone_label         String
  phone_placeholder   String
  message_placeholder String
  button              String
  user                User?   @relation(fields: [authorId], references: [id])
  authorId            String?
}

model Consultentcy_service {
  id                   String  @id @default(uuid())
  lang                 String
  title                String
  name_label           String
  surname_label        String
  phone_label          String
  email_label          String
  company_label        String
  service_label        String
  name_placeholder     String
  phone_placeholder    String
  message_placeholder  String
  services_placeholder String
  surname_placeholder  String
  email_placeholder    String
  company_placeholder  String
  button               String
  user                 User?   @relation(fields: [authorId], references: [id])
  authorId             String?
}

model Footer {
  id           String         @id @default(uuid())
  lang         String
  logo         String
  about        String
  workingHours Working_hour[]
  quickLinks   Quick_link[]
  serviceLinks Service_link[]
  user         User?          @relation(fields: [authorId], references: [id])
  authorId     String?
}

model Working_hour {
  id       String  @id @default(uuid())
  title    String
  weekdays String
  saturday String
  footer   Footer? @relation(fields: [footerId], references: [id])
  footerId String?
}

model Quick_link {
  id       String            @id @default(uuid())
  title    String
  links    Quick_link_item[]
  footer   Footer?           @relation(fields: [footerId], references: [id])
  footerId String?
}

model Quick_link_item {
  id           String      @id @default(uuid())
  label        String
  href         String
  quickLink    Quick_link? @relation(fields: [quick_linkId], references: [id])
  quick_linkId String?
}

model Service_link {
  id       String              @id @default(uuid())
  title    String
  links    Service_link_item[]
  footer   Footer?             @relation(fields: [footerId], references: [id])
  footerId String?
}

model Service_link_item {
  id             String        @id @default(uuid())
  label          String
  href           String
  serviceLink    Service_link? @relation(fields: [service_linkId], references: [id])
  service_linkId String?
}
